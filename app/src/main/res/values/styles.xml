<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="HeaderTextStyle">
        <item name="android:fontFamily">@font/montserrat_semibold</item>
        <item name="android:textSize">26sp</item>
        <item name="android:textColor">#161826</item>
    </style>
    <style name="EditTextStyle">
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#040402</item>
        <item name="android:textColorHint">@color/text_hint_color</item>
    </style>
    <style name="ButtonTextStyle">
        <item name="android:fontFamily">@font/montserrat_bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#EAEAEA</item>
    </style>
    <style name="AppBarTitleText">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:fontFamily">@font/montserrat_bold</item>
    </style>
</resources>